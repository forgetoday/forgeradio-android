<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent" android:id="@+id/RelativeLayout1">
    <TextView android:text="Show time" android:layout_width="wrap_content" android:id="@+id/scheduleTimeView" android:layout_height="wrap_content" android:textSize="15dp" android:layout_alignParentTop="true" android:layout_alignParentLeft="true" android:layout_marginLeft="15dp"></TextView>
    <TextView android:text="Show name" android:layout_width="wrap_content" android:id="@+id/scheduleNameView" android:layout_height="wrap_content" android:textSize="18dp" android:layout_below="@+id/scheduleTimeView" android:layout_alignParentRight="true" android:layout_marginRight="15dp"></TextView>
    
</RelativeLayout>
